    "ts-loader": "^7.0.4",
    "ts-mocha": "^7.0.0",
    "ts-node": "^8.10.1",
    "typescript": "^3.9.3",
    "typings": "^2.1.1",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {
    "@tabcat/orbit-db-fsstore": "^1.0.0",
    "@types/elliptic": "^6.4.12",
    "array-sort": "^1.0.0",
    "bson-objectid": "^1.3.0",
    "cids": "^0.8.3",
    "datastore-fs": "^1.1.0",
    "debug": "^4.1.1",
